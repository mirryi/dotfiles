# It was generated by processing a template.

[global]
  monitor = 0
  follow = keyboard
  geometry = "350x5-10+34"
  indicate_hidden = yes
  shrink = yes
  transparency = 0
  notification_height = 0
  separator_height = 0
  padding = 8
  horizontal_padding = 15
  frame_width = 3
  frame_color = "#1d2021"

  # Define a color for the separator.
  # possible values are:
  #  * auto: dunst tries to find a color fitting to the background;
  #  * foreground: use the same color as the foreground;
  #  * frame: use the same color as the frame;
  #  * anything else will be interpreted as a X color.
  separator_color = frame

  # Sort messages by urgency.
  sort = yes

  idle_threshold = 120
  font = {{ font.family }} {{ font.size }}
  line_height = 0
  markup = full

  # The format of the message.  Possible variables are:
  #   %a  appname
  #   %s  summary
  #   %b  body
  #   %i  iconname (including its path)
  #   %I  iconname (without its path)
  #   %p  progress value if set ([  0%] to [100%]) or nothing
  #   %n  progress value if set without any extra characters
  #   %%  Literal %
  # Markup is allowed
  format = "<b>%a:</b> %s\n%b"
  alignment = left
  show_age_threshold = 60
  word_wrap = yes
  ellipsize = middle
  ignore_newline = no
  stack_duplicates = true
  hide_duplicate_count = true
  show_indicators = yes
  icon_position = left
  max_icon_size = 40
  sticky_history = yes
  history_length = 20
  browser = /usr/bin/qutebrowser -R

  always_run_script = true

  title = Dunst
  class = Dunst
  startup_notification = false
  force_xinerama = false

[experimental]
  per_monitor_dpi = false

[shortcuts]
  close = mod1+shift+b
  close_all = mod1+ctrl+b
  history = mod1+shift+backslash
  context = mod1+ctrl+shift+backslash

[urgency_low]
  background = "{{ colors.low.background }}"
  foreground = "{{ colors.low.foreground }}"
  timeout = 5

[urgency_normal]
  background = "{{ colors.normal.background }}"
  foreground = "{{ colors.normal.foreground }}"
  timeout = 5

[urgency_critical]
  background = "{{ colors.critical.background }}"
  foreground = "{{ colors.critical.foreground }}"
  timeout = 5
