snippet today "Insert today's date"
`date +%Y-%m-%d`
endsnippet

snippet qq "quote ? quote"
"${0:${VISUAL}}"
endsnippet

snippet qqq "'?'"
'${0:${VISUAL}}'
endsnippet

snippet bb "{?}"
{${0:${VISUAL}}}
endsnippet

snippet bbb "[?]"
[${0:${VISUAL}}]
endsnippet

snippet pp "(?)"
(${0:${VISUAL}})
endsnippet
