snippet today "Insert today's date" i
`date +%Y-%m-%d`
endsnippet

snippet qq "quote ? quote" i
"${0:${VISUAL}}"
endsnippet

snippet qqq "'?'" i
'${0:${VISUAL}}'
endsnippet

snippet bb "{?}" i
{${0:${VISUAL}}}
endsnippet

snippet bbb "[?]" i
[${0:${VISUAL}}]
endsnippet

snippet pp "(?)" i
(${0:${VISUAL}})
endsnippet
